//for，while,do...while三种循环
//for（表达式1初始化；表达式判断；表达式3调整）
//#include <stdio.h>
//int main()
//{
//	int i = 0;//循环变量的一个初始化
//	while (i < 10)//判断
//	{
//		//...
//		i++//调整
//	}
//
//
//	return 0;
//}


//用for循环打印1到10
#include <stdio.h>
int main()
{
	int i = 0;//初始化判断 
	for (i = 1; i <= 10; i++)//关于分号；，如果加上分号之后就代表先循环完再输出，不加分号才会进行真正的for循环
	{
		printf("%d ", i);
	}
	return 0;
}
